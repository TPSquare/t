{
  "name": "algoscene",
  "version": "3.0.0",
  "main": "index.js",
  "scripts": {
    "build:tailwindcss": "npx @tailwindcss/cli -i ./source/styles/tailwind.css -o ./public/styles/tailwind.css",
    "build:postcss": "postcss ./source/styles/contents/*.css -d ./public/styles/contents/",
    "build": "npm-run-all build:tailwindcss build:postcss",
    "start": "node ./source/index.js",
    "dev:tailwindcss": "npx @tailwindcss/cli -i ./source/styles/tailwind.css -o ./public/styles/tailwind.css --watch",
    "dev:postcss": "postcss ./source/styles/contents/*.css -d ./public/styles/contents/ --watch",
    "dev:styles": "npm-run-all --parallel dev:tailwindcss dev:postcss",
    "dev:nodemon": "nodemon ./source/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TPSquare/AlgoScene.git"
  },
  "author": "TPSquare",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/TPSquare/AlgoScene/issues"
  },
  "homepage": "https://github.com/TPSquare/AlgoScene#readme",
  "description": "",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.4",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.4",
    "postcss-cli": "^11.0.1",
    "postcss-nested": "^7.0.2",
    "postcss-prefix-selector": "^2.1.1",
    "prettier-plugin-tailwindcss": "^0.6.11"
  }
}
